{
  "name": "schedulingsystem",
  "version": "1.0.0",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "pm2:start-development": "npx pm2 start ecosystem.config.cjs --env development",
    "pm2:start-production": "npx pm2 start ecosystem.config.cjs --env production",
    "dev": "npx tsx --env-file=.env.local --watch --no-warnings src/server.ts",
    "start-production": "npx prisma generate && node dist/src/server.js",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "build": "tsc && tsc-alias",
    "pm2:list": "npx pm2 list",
    "pm2:kill": "npx pm2 kill",
    "pm2:logs": "npx pm2 log"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "amqplib": "^0.10.9",
    "cron": "^4.3.3",
    "cron-parser": "^5.4.0",
    "dayjs": "^1.11.19",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "pm2": "^6.0.14",
    "prisma": "^7.2.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@prisma/client": "^7.2.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/amqplib": "^0.10.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.7.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
